#include <bits/stdc++.h>
using namespace std;

#define LOG(x) (cout<<x<<endl)

long long solve (long long n) {
    long long r = -1;
    for (long long k = 2; k < n; k++) {
        for (long long x = 2; x < n; x++) {

           long long target = pow(2, (k-1))*x;
           LOG(target);
           if (target == n) {
               r=x;
               break;
           } else continue;
            
        }
    }
    return r;
}

int main() {

    int t;
    cin>>t;

    long long o [t];

    for (int i = 0; i < t; i++) {

       long long n;
       cin>>n;

       // 2^(k-1) * x = n
       // output x
       //

       o[i] = solve(n);

    }

    for (int i = 0; i < t; i++) LOG(o[i]);

}
